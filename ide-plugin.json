{
	"services": [
		{
			"active": true,
			"id":"webos",
			"name":"webOS",
			"type": ["build", "test"],
			"command":"@NODE@", "params":[
				"@PLUGINDIR@/ares/server/bdWebos.js", "-l", "http", "-I", "@INSTALLDIR@", "-P", "/webos", "-p", "0", "-t", "240000"
			],
			"pluginClient": "@PLUGINURL@/ares/client/package.js",
			"useJsonp":false,
			"verbose": false,
			"respawn": false
		},
		{
			"id": "genZip",
			"sources": [
				{
				"id": "WebOS-Ports",
				"type": null
				}, 
				{
					"id": "bootplate-sample-local",
					"Xtype": "template",
					"type": null,
					"description": "Local Bootplate Sample(Local)",
					"deps": ["webos-app-config"],
					"files": [
						{
							"url": "@PLUGINDIR@/../bootplate-folder-name",
							"excluded": [	
								".git/",
								"enyo/.git/",
								"lib/enyo-sample/.git/"
							]
						}
					]
				},
				{
					"id": "bootplate-web",
					"type": "template",
					"version": "0.0.1",
					"isDefault": false,
					"description": "Basic web template for webOS - none enyo library.",
					"class":"basic",
					"deps": ["webappinfo", "webicon"],
					"files": [
						{
							"url": "@PLUGINDIR@/templates/bootplate-web"
						}
					]
				},
				{
					"id": "bootplate-moonstone-old",
					"xtype": "template",
					"type": null,
					"version": "2.5",
					"isDefault": false,
					"description": "Enyo moonstone bootplate",
					"class":"moonstone",
					"deps": ["enyoappinfo", "enyoicon"],
					"files": [
						{
							"url": "@PLUGINDIR@/templates/bootplate-moonstone",
							"excluded": [	
								".git",
								".gitignore",
								".gitmodules",
								"enyo/.git",
								"enyo/.gitignore",
								"lib/enyo-cordova/.git",
								"lib/enyo-ilib/.git",
								"lib/enyo-webos/.git",
								"lib/layout/.git",
								"lib/moonstone/.git",
								"lib/moonstone/.gitignore",
								"lib/spotlight/.git"
							]
						}
					]
				},
				{
					"id": "bootplate-moonstone",
					"type": "template",
					"version": "2.5",
					"isDefault": true,
					"description": "Enyo moonstone app",
					"class": "moonstone",
					"deps": ["enyoappinfo", "enyoicon"],
					"files": [{
						"url": "@PLUGINDIR@/templates/app-moonstone",
						"symlink": {
							"enyo": "@PLUGINDIR@/templates/bootplate-moonstone/enyo",
							"lib": "@PLUGINDIR@/templates/bootplate-moonstone/lib"
						}
					}]
				},
				{
					"id": "bootplate-2.3.0-pre.12",
					"Xtype": "template",
					"type": null,
					"version": "2.3.0-pre.12",
					"isDefault": false,
					"description": "Enyo bootplate 2.3.0 (No webOS specific library)",
					"class":"basic",
					"deps": ["webappinfo"],
					"files": [
						{
							"url": "http://enyojs.com/archive/bootplate-2.3.0-pre.12.zip",
							"prefixToRemove": "bootplate",
							"excluded": [
								"bootplate/api/"
							]
						}
					]
				},
				{
					"id": "bootplate-2.2.0",
					"type": "null",
					"version": "2.2.0",
					"description": "Enyo bootplate 2.2.0",
					"deps": ["webos-app-config"],
					"class":"basic",
					"files": [
						{
							"url": "http://enyojs.com/archive/bootplate-2.2.0.zip",
							"prefixToRemove": "bootplate",
							"excluded": [
								"bootplate/api/"
							]
						}
					]
				},
				{
					"id": "bootplate-2.1.1",
					"type": "null",
					"version": "2.1.1",
					"description": "Enyo bootplate 2.1.1",
					"deps": ["webos-app-config"],
					"class":"basic",
					"files": [
						{
							"url": "http://enyojs.com/archive/bootplate-2.1.1.zip",
							"prefixToRemove": "bootplate",
							"excluded": [
								"bootplate/api/"
							]
						}
					]
				},
				{
					"id": "webos-service-helloworld",
					"Xtype": "webosService",
					"type" : null,
					"version": "0.0.1",
					"description": "webOS Node.js Service Template",
					"files": [
						{
							"url": "http://enyojs.com/webos/webos-service-config.zip",
							"prefixToAdd": "services/com.example.helloworld"
						}
					]
				},
				{
					"id": "webos-service",
					"type": "webosService",
					"version": "0.0.1",
					"description": "webOS Node.js Service Template",
					"files": [
						{
							"url": "@PLUGINDIR@/templates/webos-service",
							"prefixToAdd": ""
						}
					],
					"filesubstitution": [
						"package.json",
						"services.json",
						"helloworld_webos_service.js",
						"helloclient.js"
					]
				},
				{
					"id": "webos-app-config",
					"Xtype": "webosConfig",
					"type": null,
					"version": "0.0.1",
					"description": "webOS Applicatin Config data",
					"files": [
						{
							"url": "http://enyojs.com/webos/webos-app-config.zip"
						}
					]
				},
				{
					"id": "webappinfo",
					"type": "appinfo",
					"version": "0.0.1",
					"description": "minimal appinfo.json for webOS Applicatin",
					"files": [
						{
							"url": "@PLUGINDIR@/templates/webappinfo"
						}
					],
					"filesubstitution": [
						"appinfo.json"
					]
				},
				{
					"id": "enyoappinfo",
					"type": "appinfo",
					"version": "0.0.1",
					"description": "appinfo.json to use built-in enyo framework on WebOS Device",
					"files": [
						{
							"url": "@PLUGINDIR@/templates/enyoappinfo"
						}
					],
					"filesubstitution": [
						"appinfo.json"
					]
				},
				{
					"id": "webicon",
					"type": "image",
					"version": "0.0.1",
					"description": "normal icon files [icon.png (80x80), largeIcon.png (130x130)]",
					"files": [
						{
							"url": "@PLUGINDIR@/templates/webicon"
						}
					]
				},
				{
					"id": "enyoicon",
					"type": "image",
					"version": "0.0.1",
					"description": "enyo icon files [icon.png (80x80), largeIcon.png (130x130)]",
					"files": [
						{
							"url": "@PLUGINDIR@/templates/enyoicon"
						}
					]
				}
			]
		}
	]
}
