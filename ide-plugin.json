{
	"services": [
		{
			"active": true,
			"id":"webos",
			"name":"webOS",
			"type": ["build", "test"],
			"command":"@NODE@", "params":[
				"@PLUGINDIR@/ares/server/main.js", "-P", "/webos", "-p", "0", "-t", "240000"
			],
			"pluginClient": "@PLUGINURL@/ares/client/package.js",
			"useJsonp":false,
			"verbose": false,
			"respawn": false
		},
		{
			"id": "genZip",
			"sources": [
				{
					"id": "bootplate-sample-local",
					"Xtype": "template",
					"type": null,
					"description": "Local Bootplate Sample(Local)",
					"deps": ["webos-app-config"],
					"files": [
						{
							"url": "@PLUGINDIR@/../bootplate-folder-name",
							"excluded": [	
								".git/",
								"enyo/.git/",
								"lib/enyo-sample/.git/"
							]
						}
					]
				},
				{
					"id": "bootplate-2.3.0-pre.5",
					"type": "template",
					"default": true,
					"description": "Enyo bootplate 2.3.0-pre.5",
					"deps": ["webos-app-config"],
					"files": [
						{
							"url": "http://enyojs.com/archive/bootplate-2.3.0-pre.5.zip",
							"prefixToRemove": "bootplate",
							"excluded": [
								"bootplate/api/"
							]
						}
					]
				},
				{
					"id": "bootplate-2.2.0",
					"type": "template",
					"description": "Enyo bootplate 2.2.0",
					"deps": ["webos-app-config"],
					"files": [
						{
							"url": "http://enyojs.com/archive/bootplate-2.2.0.zip",
							"prefixToRemove": "bootplate",
							"excluded": [
								"bootplate/api/"
							]
						}
					]
				},
				{
					"id": "bootplate-2.1.1",
					"type": "template",
					"description": "Enyo bootplate 2.1.1",
					"deps": ["webos-app-config"],
					"files": [
						{
							"url": "http://enyojs.com/archive/bootplate-2.1.1.zip",
							"prefixToRemove": "bootplate",
							"excluded": [
								"bootplate/api/"
							]
						}
					]
				},
				{
					"id": "webos-service",
					"type": "webosService",
					"description": "Hello World webOS Service 1.0.0",
					"files": [
						{
							"url": "http://enyojs.com/webos/webos-service-config.zip",
							"prefixToAdd": "services/com.yourdomain.hello.service"
						}
					]
				},
				{
					"id": "webos-app-config",
					"type": "webosConfig",
					"description": "webOS Applicatin Config data",
					"files": [
						{
							"url": "http://enyojs.com/webos/webos-app-config.zip"
						}
					]
				}
			]
		}
	]
}
