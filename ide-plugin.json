{
	"services": [
		{
			"active": true,
			"id":"webos",
			"name":"webOS",
			"type": ["build", "test"],
			"command":"@NODE@", "params":[
				"@PLUGINDIR@/ares/server/bdWebos.js", "-l", "http", "-I", "@INSTALLDIR@", "-P", "/webos", "-p", "0", "-t", "240000"
			],
			"pluginClient": "@PLUGINURL@/ares/client/package.js",
			"useJsonp":false,
			"verbose": false,
			"respawn": false
		},
		{
			"id": "genZip",
			"sources": [
				{
					"id": "bootplate-web",
					"type": "template",
					"version": "0.0.1",
					"isDefault": false,
					"description": "Basic web template for webOS - none enyo library.",
					"class":"basic",
					"deps": ["webappinfo", "webicon"],
					"files": [
						{
							"url": "@PLUGINDIR@/templates/bootplate-web"
						}
					]
				},
				{
					"id": "bootplate-moonstone",
					"type": "template",
					"version": "2.5",
					"isDefault": true,
					"description": "Enyo moonstone app",
					"class": "moonstone",
					"deps": ["enyoappinfo", "enyoicon"],
					"files": [{
						"url": "@PLUGINDIR@/templates/app-moonstone"
					}]
				},
				{
					"id": "sdl_basic",
					"type": "native",
					"version": "0.0.1",
					"isDefault": false,
					"description": "SDL Basic UI",
					"class":"native_app",
					"deps": ["nativeappinfo", "nativeicon"],
					"files": [{
						"url": "@PLUGINDIR@/templates/native/2DGames"
					}]
				},
				{
					"id": "sdl_media",
					"type": "native",
					"version": "0.0.1",
					"isDefault": false,
					"description": "SDL Media App",
					"class":"native_app",
					"deps": ["nativeappinfo", "nativeicon"],
					"files": [{
						"url": "@PLUGINDIR@/templates/native/MediaApp"
					}]
				},
				{
					"id": "sdl_opengles1",
					"type": "native",
					"version": "0.0.1",
					"isDefault": false,
					"description": "SDL with OpenGLES v1.1",
					"class":"native_app",
					"deps": ["nativeappinfo", "nativeicon"],
					"files": [{
						"url": "@PLUGINDIR@/templates/native/OpenGLESv1.1Project"
					}]
				},
				{
					"id": "sdl_opengles2",
					"type": "native",
					"version": "0.0.1",
					"isDefault": false,
					"description": "SDL with OpenGLES v2.0",
					"class":"native_app",
					"deps": ["nativeappinfo", "nativeicon"],
					"files": [{
						"url": "@PLUGINDIR@/templates/native/OpenGLESv2.0Project"
					}]
				},
				{
					"id": "webos-service",
					"type": "webosService",
					"version": "0.1",
					"description": "webOS Node.js Service Template",
					"files": [
						{
							"url": "@PLUGINDIR@/templates/webos-service",
							"prefixToAdd": ""
						}
					]
				},
				{
					"id": "native-service",
					"type": "nativeService",
					"version": "0.1",
					"description": "webOS Native Service Template",
					"files": [
						{
							"url": "@PLUGINDIR@/templates/native-service",
							"prefixToAdd": ""
						}
					]
				},
				{
					"id": "webappinfo",
					"type": "appinfo",
					"version": "0.0.1",
					"description": "minimal appinfo.json for webOS Application",
					"files": [
						{
							"url": "@PLUGINDIR@/templates/webappinfo"
						}
					],
					"filesubstitution": [
						"appinfo.json"
					]
				},
				{
					"id": "enyoappinfo",
					"type": "appinfo",
					"version": "0.0.1",
					"description": "appinfo.json to use built-in enyo framework on WebOS Device",
					"files": [
						{
							"url": "@PLUGINDIR@/templates/enyoappinfo"
						}
					],
					"filesubstitution": [
						"appinfo.json"
					]
				},
				{
					"id": "nativeappinfo",
					"type": "appinfo",
					"version": "0.0.1",
					"description": "appinfo.json for native application",
					"files": [
						{
							"url": "@PLUGINDIR@/templates/nativeappinfo",
							"prefixToAdd": "pkg"
						}
					],
					"filesubstitution": [
						"appinfo.json"
					]
				},
				{
					"id": "webicon",
					"type": "image",
					"version": "0.0.1",
					"description": "normal icon files [icon.png (80x80), largeIcon.png (130x130)]",
					"files": [
						{
							"url": "@PLUGINDIR@/templates/webicon"
						}
					]
				},
				{
					"id": "enyoicon",
					"type": "image",
					"version": "0.0.1",
					"description": "enyo icon files [icon.png (80x80), largeIcon.png (130x130)]",
					"files": [
						{
							"url": "@PLUGINDIR@/templates/enyoicon"
						}
					]
				},
				{
					"id": "nativeicon",
					"type": "image",
					"version": "0.0.1",
					"description": "native app icon files [icon.png (80x80), largeIcon.png (130x130)]",
					"files": [
						{
							"url": "@PLUGINDIR@/templates/nativeicon",
							"prefixToAdd": "pkg"
						}
					]
				},
				{
					"id": "moonstone-2.5",
					"type": "bootplate",
					"version": "2.5",
					"onDevice": true,
					"description": "bootplate-moonstone",
					"files": [{
						"url": "@PLUGINDIR@/templates/empty",
						"symlink": {
							"enyo": "@PLUGINDIR@/templates/bootplate-moonstone/enyo",
							"lib": "@PLUGINDIR@/templates/bootplate-moonstone/lib"
						}
					}]
				},
				{
					"id": "moonstone-2.3",
					"type": "bootplate",
					"version": "2.3",
					"onDevice": true,
					"description": "bootplate-moonstone",
					"files": [{
						"url": "@PLUGINDIR@/templates/empty",
						"symlink": {
							"enyo": "@PLUGINDIR@/templates/bootplate-moonstone-2.3/enyo",
							"lib": "@PLUGINDIR@/templates/bootplate-moonstone-2.3/lib"
						}
					}]
				}
			]
		}
	]
}
